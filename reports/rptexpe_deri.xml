<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE REPORT SYSTEM "PHPReport.dtd">
<REPORT MARGINWIDTH="5" MARGINHEIGHT="5">
   <TITLE>HOJA DE TRAMITE</TITLE>
   <BACKGROUND_COLOR>#FFFFFF</BACKGROUND_COLOR>
	<CSS>rptreports.css</CSS>   
	<NO_DATA_MSG>No existen datos, verifique su consulta...</NO_DATA_MSG>
	<DOCUMENT>
		<FOOTER>
			<ROW>
				<COL CELLCLASS="FOOTER" TEXTCLASS="BOLDRED" TYPE="RAW_EXPRESSION" ALIGN="LEFT">autor()</COL>			
				<COL COLSPAN="11" CELLCLASS="FOOTER" TEXTCLASS="BOLDRED" TYPE="EXPRESSION" ALIGN="RIGHT">"Total Registros: ".$this->getRowCount()</COL>
			</ROW>
		</FOOTER>
	</DOCUMENT>
	<PAGE BORDER="1" SIZE="0" CELLSPACING="0" CELLPADDING="0">
		<HEADER>
			<ROW>
				<COL CELLCLASS="HEADER" COLSPAN="12" TYPE="RAW_EXPRESSION" >
				cabecera($this->getParameter("titulo"),"Reportes")
				</COL>
			</ROW>	
		</HEADER>
	</PAGE>
	<GROUPS>
			<GROUP NAME="group" RESET_SUPPRESS_ON_PAGEBREAK="TRUE" REPRINT_HEADER_ON_PAGEBREAK="TRUE">
				<HEADER>
					<ROW>
						<COL CELLCLASS="HEADER" TEXTCLASS="BOLD" ALIGN="CENTER">REGISTRO</COL>
						<COL CELLCLASS="HEADER" TEXTCLASS="BOLD" WIDTH="55" ALIGN="CENTER">FECHA</COL>
						<COL CELLCLASS="HEADER" TEXTCLASS="BOLD" ALIGN="CENTER">EXPEDIENTE</COL>
						<COL CELLCLASS="HEADER" TEXTCLASS="BOLD" WIDTH="55" ALIGN="CENTER">FECHA DOC.</COL>
						<COL CELLCLASS="HEADER" TEXTCLASS="BOLD" ALIGN="CENTER">FOL.</COL>
						<COL CELLCLASS="HEADER" TEXTCLASS="BOLD" ALIGN="CENTER">UNIDAD ORG.</COL>
						<COL CELLCLASS="HEADER" TEXTCLASS="BOLD" ALIGN="CENTER">DETALLE</COL>												
						<COL CELLCLASS="HEADER" TEXTCLASS="BOLD" ALIGN="CENTER">FIRMA</COL>
						<COL CELLCLASS="HEADER" TEXTCLASS="BOLD" ALIGN="CENTER">CARGO</COL>
						<COL CELLCLASS="HEADER" TEXTCLASS="BOLD" ALIGN="CENTER">ASUNTO</COL>												
					</ROW>
				</HEADER>

					<GROUP NAME="destino" EXPRESSION="destino" RESET_SUPPRESS_ON_PAGEBREAK="TRUE" >
						<HEADER>
							<ROW>
								<COL ALIGN="RIGHT"  TEXTCLASS="BOLD" COLSPAN="2" >Oficina Destino: </COL>
								<COL TYPE="EXPRESSION" TEXTCLASS="BOLD" COLSPAN="8">$this->getValue("destino")</COL>
							</ROW>
						</HEADER>				
						<FIELDS>
							<ROW>
								<COL TYPE="FIELD" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" ALIGN="LEFT">expe_id</COL>
								<COL TYPE="FIELD" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" ALIGN="LEFT">oper_fecha</COL>
								<COL TYPE="FIELD" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" ALIGN="LEFT">expediente</COL>						
								<COL TYPE="FIELD" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" ALIGN="LEFT">expe_fecha_doc</COL>
								<COL TYPE="FIELD" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" ALIGN="LEFT">expe_folios</COL>
								<COL TYPE="FIELD" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" ALIGN="LEFT">dependencia</COL>
								<COL TYPE="FIELD" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" ALIGN="LEFT">expe_depe_detalle</COL>
								<COL TYPE="FIELD" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" ALIGN="LEFT">expe_firma</COL>
								<COL TYPE="FIELD" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" ALIGN="LEFT">expe_cargo</COL>						
								<COL TYPE="FIELD" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" ALIGN="LEFT">expe_asunto</COL>
							</ROW>
						</FIELDS>
				</GROUP>	
			</GROUP>	
	</GROUPS>
</REPORT>
